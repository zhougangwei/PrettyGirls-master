<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="coder.aihui.ui.bd.FormActivity">


    <include
        android:id="@+id/title"
        layout="@layout/include_small_title"></include>

    <include
        layout="@layout/include_title_text"></include>

    <include layout="@layout/icon_back">
    </include>

    <include
        android:id="@+id/ll_search"
        layout="@layout/include_search_ll"
        android:layout_width="673px"
        android:layout_height="88px"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="171px"
        android:background="@drawable/ic_zhengfangxing"
        android:orientation="horizontal"></include>

    <android.support.v7.widget.RecyclerView
        android:id="@+id/rv"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/ll_search"
        android:layout_marginTop="12px"></android.support.v7.widget.RecyclerView>


</RelativeLayout>
