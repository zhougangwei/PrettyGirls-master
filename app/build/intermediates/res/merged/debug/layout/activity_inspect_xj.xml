<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"

    tools:context="coder.aihui.ui.inspectxj.InspectXJActivity">


    <include layout="@layout/include_mid_title"></include>

    <include
        android:id="@+id/tv_title"
        layout="@layout/include_title_text"></include>

    <include layout="@layout/icon_back"></include>

    <include layout="@layout/include_config"></include>

    <include layout="@layout/include_updown"></include>

    <include layout="@layout/include_sz"></include>

    <include layout="@layout/include_search_ll"></include>




    <include
        layout="@layout/include_tab"></include>

    <ImageView
        android:id="@+id/line"
        android:layout_width="match_parent"
        android:layout_height="8px"
        android:layout_below="@+id/tb"
        android:background="#F5F5F5"
        />

    <android.support.v4.view.ViewPager
        android:id="@+id/vp"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/line">
    </android.support.v4.view.ViewPager>

    <coder.aihui.widget.CircleImageView
        app:border_color="#ffffffff"
        android:id="@+id/fab_xj"
        android:layout_width="104px"
        android:layout_height="104px"
        android:layout_marginLeft="576px"
        android:layout_marginTop="879px"
        app:border_width="1px"
        android:src="@mipmap/ksxj"
        app:layout_auto_baseheight="width"/>
    <include layout="@layout/layout_pagenum"/>


</RelativeLayout>
